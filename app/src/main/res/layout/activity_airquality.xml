<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.rihuisoft.loginmode.utils.ViewPagerIndicator
        android:id="@+id/id_indicator"
        android:layout_width="match_parent"
        android:layout_height="162px"
        android:background="@color/white"
        android:orientation="horizontal"
        >

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon1"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_01m_sel"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />

            <ImageView
                android:id="@+id/iv_cutting1"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"

                />
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon2"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_02m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />
            <ImageView
                android:id="@+id/iv_cutting2"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </com.zhy.autolayout.AutoRelativeLayout>
        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon3"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_03m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />
            <ImageView
                android:id="@+id/iv_cutting3"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:visibility="gone"
                />
        </com.zhy.autolayout.AutoRelativeLayout>
        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon4"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_04m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />
            <ImageView
                android:id="@+id/iv_cutting4"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon5"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_05m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />
            <ImageView
                android:id="@+id/iv_cutting5"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon6"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_06m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />
            <ImageView
                android:id="@+id/iv_cutting6"
                android:layout_width="@dimen/dimen_px_30"
                android:layout_height="match_parent"
                android:src="@drawable/img_cutting"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                />
        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >
            <ImageView
                android:id="@+id/iv_tabicon7"
                android:layout_width="120px"
                android:layout_height="@dimen/dimen_px_90"
                android:src="@drawable/ico_air_07m_nor"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dimen_px_30"
                android:layout_marginBottom="@dimen/dimen_px_30"
                />

        </com.zhy.autolayout.AutoRelativeLayout>
    </com.rihuisoft.loginmode.utils.ViewPagerIndicator>
    <android.support.v4.view.ViewPager
        android:id="@+id/vp_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/id_indicator"
        ></android.support.v4.view.ViewPager>

</com.zhy.autolayout.AutoRelativeLayout>
